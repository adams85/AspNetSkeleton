{
  "CoreSettings": {
    //"ShutDownTimeOut": "00:01:00",
    "UIBaseUrl": "http://localhost:10801",
    "AdminMailTo": [ "admin@example.com" ]
  },

  "UISettings": {
    "ListenUrl": "http://localhost:10801",
    "ReverseProxies": [],

    "EnableResponseMinification": false,
    "EnableResponseCompression": false,
    "EnableResponseCaching": false,
    //"CacheHeaderMaxAge": "7.00:00:00",

    "EnableLocalization": false,
    //"DefaultCulture": "en-US",

    "EnableTheming": false,
    //"DefaultTheme": "Dark",

    "EnableRegistration": true,
    //"PasswordTokenExpiration": "1.00:00:00",
    "DefaultDeviceLimit": 2
  },

  "Authentication": {
    "Cookie": {
      "HttpOnly": true,
      "Expiration": "30.0:0:0"
    },
    "LoginPath": "/Account/Login",
    "LogoutPath": "/Account/Logout",
    "AccessDeniedPath": "/Account/AccessDenied",
    "SlidingExpiration": true
  },

  "Logging": {
    "File": {
      "LogLevel": {
        "Default": "Warning"
      },
      "BasePath": "Logs",
      "EnsureBasePath": true,
      "FileNameMappings": {
        "AspNetSkeleton.UI.Hosting": "host.log",
        "AspNetSkeleton": "app.log"
      }
    }
  }
}
